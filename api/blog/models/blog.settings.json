{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "blog",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "gatsby",
        "javascript",
        "react",
        "html",
        "css",
        "rust",
        "c",
        "php",
        "symfony",
        "laravel",
        "node",
        "express",
        "dino",
        "jam",
        "haskell",
        "lisp",
        "elm",
        "docker",
        "aws",
        "kubernetes",
        "sql",
        "mongo",
        "python",
        "flask",
        "django",
        "wordpress",
        "sass"
      ],
      "required": true,
      "default": "gatsby"
    },
    "desc": {
      "type": "text",
      "required": true
    },
    "date": {
      "type": "date"
    }
  }
}
